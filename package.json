{
  "name": "ts-antd-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.8.1",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts-antd": "2.15.1",
    "serve": "^9.6.0"
  },
  "scripts": {
    "start": "react-scripts-ts-antd start",
    "build": "sh -ac './.env.${REACT_APP_ENV}; react-scripts-ts-antd build",
    "build:development": "set REACT_APP_ENV=development yarn build",
    "build:staging": "set REACT_APP_ENV=staging && yarn build",
    "build:production": "set REACT_APP_ENV=production && yarn build",
    "test": "react-scripts-ts-antd build",
    "eject": "react-scripts-ts-antd eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "react-app-env": "^1.2.2",
    "typescript": "^3.0.1"
  }
}
